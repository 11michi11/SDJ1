public class Person {
	
	private String name;
	private MyDate birthday;
	
	
	public Person(String name, MyDate birthday) {
		this.name = name;
		this.birthday = birthday;
	}
	
	public Person(MyDate birthday) {
		this.birthday = birthday;
		this.name = "No name";
	}
	

	public String getName() {
		return name;
	}
	
	public MyDate getBirthday() {
		return birthday;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
}
